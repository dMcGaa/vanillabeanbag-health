{"pageProps":{"note":{"id":"f8ce4kj5q8xb03nxo74pdqf","title":"Cracker - Almond Flour","desc":"","updated":1648305484666,"created":1648305484666,"custom":{},"fname":"recipe.cracker-almond-flour","type":"note","vault":{"fsPath":"vault"},"contentHash":"388488df690cbc14b22df28343c80aa9","links":[{"from":{"fname":"root","vaultName":"vault"},"type":"backlink","position":{"start":{"line":8,"column":3,"offset":158},"end":{"line":8,"column":34,"offset":189},"indent":[]},"value":"recipe.cracker-almond-flour","alias":"recipe.cracker-almond-flour"},{"from":{"fname":"recipe.keto-friendly-list","vaultName":"vault"},"type":"backlink","position":{"start":{"line":9,"column":3,"offset":456},"end":{"line":9,"column":34,"offset":487},"indent":[]},"value":"recipe.cracker-almond-flour","alias":"recipe.cracker-almond-flour"}],"anchors":{"quick-info":{"type":"header","text":"Quick info","value":"quick-info","line":8,"column":0,"depth":2},"ingredients-dry":{"type":"header","text":"Ingredients (dry)","value":"ingredients-dry","line":19,"column":0,"depth":2},"ingredients":{"type":"header","text":"Ingredients","value":"ingredients","line":31,"column":0,"depth":2},"directions":{"type":"header","text":"Directions","value":"directions","line":36,"column":0,"depth":2}},"children":[],"parent":"co5sgrbal8e7wntd8z2iw0h","data":{}},"body":"<h1 id=\"cracker---almond-flour\"><a aria-hidden=\"true\" class=\"anchor-heading\" href=\"#cracker---almond-flour\"><svg aria-hidden=\"true\" viewBox=\"0 0 16 16\"><use xlink:href=\"#svg-link\"></use></svg></a>Cracker - Almond Flour</h1>\n<h2 id=\"quick-info\"><a aria-hidden=\"true\" class=\"anchor-heading\" href=\"#quick-info\"><svg aria-hidden=\"true\" viewBox=\"0 0 16 16\"><use xlink:href=\"#svg-link\"></use></svg></a>Quick info</h2>\n<ul>\n<li>\n<p>1 square serving (61 kcal)</p>\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n<table><thead><tr><th>macro</th><th>ratio</th><th>grams</th><th>calories</th></tr></thead><tbody><tr><td>fat</td><td>74%</td><td>5g</td><td>45 kcal</td></tr><tr><td>protein</td><td>19.5%</td><td>3g</td><td>12 kcal</td></tr><tr><td>net carbs</td><td>6.5%</td><td>1g</td><td>4 kcal</td></tr></tbody></table>\n</li>\n<li>\n<p>48 crackers</p>\n</li>\n<li>\n<p>oven 300 F</p>\n</li>\n</ul>\n<h2 id=\"ingredients-dry\"><a aria-hidden=\"true\" class=\"anchor-heading\" href=\"#ingredients-dry\"><svg aria-hidden=\"true\" viewBox=\"0 0 16 16\"><use xlink:href=\"#svg-link\"></use></svg></a>Ingredients (dry)</h2>\n<ul>\n<li>2 cups almond flour</li>\n<li>(1/2 cup almond meal, optional)</li>\n<li>1/2 cup flax meal (ground seeds)\n<ul>\n<li>substitute with 1/2 cup tiger nuts</li>\n</ul>\n</li>\n<li>3 Tbsp sesame seeds</li>\n<li>2 Tbsp dried onions (flakes)</li>\n<li>1/2 cup parmesan cheese</li>\n<li>1/4-1/2 tsp salt (pink or sea)</li>\n<li>2 Tbsp artichoke inulin (or chickory root)</li>\n<li>1/2 tsp baking soda (optional)</li>\n</ul>\n<h2 id=\"ingredients\"><a aria-hidden=\"true\" class=\"anchor-heading\" href=\"#ingredients\"><svg aria-hidden=\"true\" viewBox=\"0 0 16 16\"><use xlink:href=\"#svg-link\"></use></svg></a>Ingredients</h2>\n<ul>\n<li>1 large egg</li>\n<li>2 Tbsp oil</li>\n<li>1/2 cup water</li>\n</ul>\n<h2 id=\"directions\"><a aria-hidden=\"true\" class=\"anchor-heading\" href=\"#directions\"><svg aria-hidden=\"true\" viewBox=\"0 0 16 16\"><use xlink:href=\"#svg-link\"></use></svg></a>Directions</h2>\n<ol>\n<li>mix dry ingredients</li>\n<li>mix wet ingredients</li>\n<li>combine dry with wet ingredients</li>\n<li>divide in half</li>\n<li>place one half on sheet parchment paper</li>\n<li>cover with plastic wrap to prevent from sticking to fingers or rolling pin</li>\n<li>flatten between parchment and plastic wrap, flatten to round or rectangular</li>\n<li>flatten to 1/4-1/8in thick</li>\n<li>remove plastic wrap</li>\n<li>leave dough on parchment paper</li>\n<li>cut into little cracker squares with pizza cutter</li>\n<li>sprinkle with sesame seeds, salt, if desired</li>\n<li>bake 300 F, 30 minutes until light brown or firm</li>\n</ol>\n<hr>\n<strong>Backlinks</strong>\n<ul>\n<li><a href=\"/notes/b4sxhufsiue6pv9evqo2mp8\">VanillaBeanBag - Home</a></li>\n<li><a href=\"/notes/nvix0qdo6d2yzkjdseyu8y3\">Keto Friendly List</a></li>\n</ul>","noteIndex":{"id":"b4sxhufsiue6pv9evqo2mp8","title":"VanillaBeanBag - Home","desc":"","updated":1648243484436,"created":1648243484437,"custom":{"nav_order":0,"permalink":"/"},"fname":"root","type":"note","vault":{"fsPath":"vault"},"contentHash":"71fedaece817d49cb1127d0c5f9a8a22","links":[{"type":"wiki","from":{"fname":"root","id":"b4sxhufsiue6pv9evqo2mp8","vaultName":"vault"},"value":"recipe.keto-friendly-list","alias":"recipe.keto-friendly-list","position":{"start":{"line":6,"column":3,"offset":83},"end":{"line":6,"column":32,"offset":112},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"recipe.keto-friendly-list"}},{"type":"wiki","from":{"fname":"root","id":"b4sxhufsiue6pv9evqo2mp8","vaultName":"vault"},"value":"recipe.cookie-tiger-nut-almond-flour","alias":"recipe.cookie-tiger-nut-almond-flour","position":{"start":{"line":7,"column":3,"offset":115},"end":{"line":7,"column":43,"offset":155},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"recipe.cookie-tiger-nut-almond-flour"}},{"type":"wiki","from":{"fname":"root","id":"b4sxhufsiue6pv9evqo2mp8","vaultName":"vault"},"value":"recipe.cracker-almond-flour","alias":"recipe.cracker-almond-flour","position":{"start":{"line":8,"column":3,"offset":158},"end":{"line":8,"column":34,"offset":189},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"recipe.cracker-almond-flour"}},{"type":"wiki","from":{"fname":"root","id":"b4sxhufsiue6pv9evqo2mp8","vaultName":"vault"},"value":"info.macronutrients","alias":"info.macronutrients","position":{"start":{"line":11,"column":3,"offset":201},"end":{"line":11,"column":26,"offset":224},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"info.macronutrients"}}],"anchors":{"welcome-to-vanillabeanbag":{"type":"header",